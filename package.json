    "globals": "^16.3.0",
    "rimraf": "^6.1.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3",
    "vite-plugin-node": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4"
